{
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "extends": [
    "defaults",
    "plugin:react/recommended"
  ],
  "globals": {
    "expect": true,
    "shallow": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": 0,
    "no-trailing-spaces": 1,
    "quotes": [1, "double"],
    "semi": 1,
    "react/prop-types": 0
  }
}
